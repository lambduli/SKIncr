(module Main imports (StdLib Foo))

(vars (
  (let X 20)
))

(functions (
  (function main ()
    (begin
      (assert! (== (getX) 10) "(== (getX) 10)")
      (assert! (== X 20) "(== X 20)")

      (setX 30)
      (set! X 40)

      (assert! (== (getX) 30) "(== (getX) 30)")
      (assert! (== X 40) "(== X 40)")
      0
    )
  )
))
